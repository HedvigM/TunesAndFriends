{
  "name": "nextjs-typescript-starter",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3500",
    "build": "next build",
    "vercel-build": "prisma generate && prisma db push && next build",
    "start": "next start",
    "what": "ECHO Package system: npm",
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "next lint",
    "validate-env": "node scripts/validate-env.js",
    "predev": "npm run validate-env",
    "prisma:pull:local": "dotenv -e .env.local -- npx prisma db pull",
    "prisma:push:local": "dotenv -e .env.local -- npx prisma db push",
    "prisma:generate:local": "dotenv -e .env.local -- npx prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.local -- prisma migrate dev"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.8.0",
    "@auth0/nextjs-auth0": "^3.8.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@prisma/client": "^6.19.0",
    "abcjs": "^6.5.2",
    "clsx": "^2.1.1",
    "dotenv": "^16.0.2",
    "dotenv-cli": "^6.0.0",
    "next": "^16.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sass": "^1.93.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.31.0",
    "eslint-config-next": "^16.0.1",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.0.6",
    "prisma": "^6.19.0",
    "typescript": "^5.9.3"
  }
}
